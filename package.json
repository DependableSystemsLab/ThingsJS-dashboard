{
  "name": "things-js-dashboard",
  "version": "0.0.1",
  "description": "Web Dashboard for ThingsJS",
  "main": "index.js",
  "scripts": {
    "test": "mocha test --exit",
    "dev-client": "node node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.dev.js",
    "dev-server": "nodemon ./src/server/index.js",
    "dev": "npm-run-all --parallel dev-client dev-server",
    "build": "node node_modules/webpack/bin/webpack.js --config webpack.prod.js --progress",
    "serve": "node ./src/server/index.js",
    "start": "npm-run-all build serve"
  },
  "keywords": [
    "Internet",
    "of",
    "Things",
    "IoT",
    "Edge",
    "Computing",
    "Distributed",
    "Computing"
  ],
  "author": "Dependable Systems Lab, University of British Columbia",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "npm-run-all": "^4.1.3",
    "react-hot-loader": "^4.3.4",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "d3": "^5.5.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "helmet": "^3.13.0",
    "react": "^16.4.1",
    "react-ace": "^6.1.4",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.3.0"
  }
}